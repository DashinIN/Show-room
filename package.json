{
    "name": "anacondazgrid",
    "version": "2.0.1",
    "description": "My proj",
    "main": "docs/index.html",
    "author": "DashinIN",
    "scripts": {
        "build:sass": "sass  --no-source-map src/sass:docs/css",
        "copy:img": "copyfiles -u 1 ./src/img/**/* docs",
        "copy:html": "copyfiles -u 1 ./src/*.html docs",
        "copy:script": "copyfiles -u 1 ./src/script/*.js docs",
        "copy": "npm-run-all --parallel copy:*",
        "watch:img": "onchange \"src/img/**/*\" -- npm run copy:img",
        "watch:html": "onchange \"src/*.html\" -- npm run copy:html",
        "watch:script": "onchange \"src/script/*.js\" -- npm run copy:script",
        "watch:sass": "sass  --no-source-map --watch src/sass:docs/css",
        "watch": "npm-run-all --parallel watch:*",
        "serve": "browser-sync start --server docs --files docs",
        "start": "npm-run-all copy --parallel watch serve",
        "build": "npm-run-all copy:html build:*",
        "postbuild": "postcss docs/css/**/*.css -u autoprefixer cssnano -r --no-map"
    },
    "dependencies": {
        "autoprefixer": "^10.4.2",
        "copyfiles": "^2.4.1",
        "cssnano": "^5.0.17",
        "gsap": "^3.10.4",
        "locomotive-scroll": "^4.1.4",
        "npm-run-all": "^4.1.5",
        "onchange": "^7.1.0",
        "postcss-cli": "^9.1.0"
    },
    "devDependencies": {
        "browser-sync": "^2.27.10",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^8.0.0",
        "gulp-csso": "^4.0.1",
        "gulp-file-include": "^2.3.0",
        "gulp-group-css-media-queries": "^1.2.2",
        "gulp-htmlmin": "^5.0.1",
        "gulp-imagemin": "^7.1.0",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^5.1.0",
        "gulp-shorthand": "^1.1.0",
        "gulp-terser": "^2.1.0",
        "sass": "^1.54.0"
    }
}
